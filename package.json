{
  "name": "fuga-music-catalog",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm --filter @fuga-catalog/api dev\" \"pnpm --filter @fuga-catalog/web dev\"",
    "dev:api": "pnpm --filter @fuga-catalog/api dev",
    "dev:web": "pnpm --filter @fuga-catalog/web dev",
    "build": "pnpm -r build",
    "type-check": "pnpm -r type-check",
    "test": "pnpm -r test",
    "test:api": "pnpm --filter @fuga-catalog/api test",
    "test:web": "pnpm --filter @fuga-catalog/web test",
    "db:generate": "pnpm --filter @fuga-catalog/api db:generate",
    "db:migrate": "pnpm --filter @fuga-catalog/api db:migrate",
    "db:push": "pnpm --filter @fuga-catalog/api db:push",
    "db:seed": "pnpm --filter @fuga-catalog/api db:seed",
    "db:studio": "pnpm --filter @fuga-catalog/api db:studio",
    "db:setup": "pnpm db:generate && pnpm db:push && pnpm --filter @fuga-catalog/api db:seed"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=8.0.0"
  }
}